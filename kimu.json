{
    "name":"my protocol",
    "version":1,
    "dataModel":"mom",
    "dialogs":[
               {
               "grouped": true,
               "title": "Bird Groups",
               "controllerName": "JsonDataSampleController",
               "sections": [
                            { "title":"On the water",
                            "elements":[
                                        {"type":"QDecimalElement", "key":"wh", "bind":"numberValue:wh", "title":"On hold"},
                                        {"type":"QDecimalElement", "key":"wm", "bind":"numberValue:wm", "title":"Marbled"},
                                        {"type":"QDecimalElement", "key":"wk", "bind":"numberValue:wk", "title":"Kitlitz"},
                                        {"type":"QDecimalElement", "key":"wu", "bind":"numberValue:wu", "title":"Unidentified"},
                                        ]
                            },
                            { "title":"Flying",
                            "elements":[
                                        {"type":"QDecimalElement", "key":"fh", "bind":"numberValue:fh", "title":"On hold"},
                                        {"type":"QDecimalElement", "key":"fm", "bind":"numberValue:fm", "title":"Marbled"},
                                        {"type":"QDecimalElement", "key":"fk", "bind":"numberValue:fk", "title":"Kitlitz"},
                                        {"type":"QDecimalElement", "key":"fu", "bind":"numberValue:fu", "title":"Unidentified"},
                                        ]
                            } ,
                            { "title":"Testing",
                            "elements":[
                                        {"type":"QButtonElement", "key":"btn1", "title":"Show Values", "controllerAction":"showValuesFromForm:"},
                                        {"type":"QButtonElement", "key":"btn2", "title":"Save Values", "controllerAction":"saveValuesFromForm:"},
                                        {"type":"QButtonElement", "key":"btn3", "title":"Show Saved State", "controllerAction":"showSavedState:"},
                                        {"type":"QButtonElement", "key":"btn4", "title":"Restore Saved Values", "controllerAction":"restoreValuesToForm:"},
                                        ]
                            } ,
                            ]
               },
               {
               "grouped": true,
               "title": "Mission Properties",
               "controllerName": "JsonDataSampleController",
               "sections": [
                            { "title":"remove this header",
                            "elements":[
                                        {"type":"QRadioElement", "key":"t", "bind":"selected:t,selectedItem:ti", "title":"Transector", "items":["101", "102", "121"], "selected":2},
                                        {"type":"QEntryElement", "key":"v1", "bind":"textValue:v1", "title":"Vessel", "placeholder":"Name"},
                                        {"type":"QEntryElement", "key":"r", "bind":"textValue:r", "title":"Recorder", "placeholder":"Initials"},
                                        {"type":"QEntryElement", "key":"o1", "bind":"textValue:o1", "title":"Observer #1", "placeholder":"Initials"},
                                        {"type":"QEntryElement", "key":"o2", "bind":"textValue:o2", "title":"Observer #2", "placeholder":"Initials"},
                                        {"type":"QRadioElement", "key":"w", "bind":"selected:w,selectedItem:wi", "title":"Weather", "items":["<50% clouds", ">50% clouds", "Fog", "Mist", "Rain"], "selected":2},
                                        {"type":"QRadioElement", "key":"v2", "bind":"selected:v2,selectedItem:v2i", "title":"Visibility", "items":["<250m", "250-500m", ">500m"], "selected":2},
                                        {"type":"QRadioElement", "key":"b", "bind":"selected:b,selectedItem:bi", "title":"Beaufort", "items":["0: Glossy", "1: Ripples", "2: Few whitecaps", "3: Scattered whitecaps", "4: Spray and whitecaps"], "selected":0},
                                        {"type":"QBooleanElement", "key":"o3", "bind":"boolValue:o3", "title":"Observing", "boolValue":0},
                                        {"type":"QDecimalElement", "key":"xx", "bind":"numberValue:xx", "title":"T.B.D."},
                                        ]
                            },
                            { "title":"Testing",
                            "elements":[
                                        {"type":"QButtonElement", "key":"btn1", "title":"Show Values", "controllerAction":"showValuesFromForm:"},
                                        {"type":"QButtonElement", "key":"btn2", "title":"Save Values", "controllerAction":"saveValuesFromForm:"},
                                        {"type":"QButtonElement", "key":"btn3", "title":"Show Saved State", "controllerAction":"showSavedState:"},
                                        {"type":"QButtonElement", "key":"btn4", "title":"Restore Saved Values", "controllerAction":"restoreValuesToForm:"},
                                        ]
                            } ,
                            ]
               },
               ],
}
